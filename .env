# API keys for language models
OPENAI_API_KEY=your_openai_api_key_here
# Default scope settings (optional)
DEFAULT_DOMAINS=example.com,test.org
DEFAULT_IP_RANGES=192.168.1.0/24

# Tool configurations
NMAP_PATH=/usr/bin/nmap
GOBUSTER_PATH=/usr/local/bin/gobuster
FFUF_PATH=/usr/local/bin/ffuf

# Scan settings
MAX_TASK_RETRIES=3
TASK_TIMEOUT=300  # seconds

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=pipeline.log